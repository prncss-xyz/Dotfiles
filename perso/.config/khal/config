[calendars]

[[princesse_xyz_calendars_local]]
path = ~/Media/calendars/princesse_xyz/*
type = discover

[[princesse_xyz_contacts_local]]
path = ~/Media/contacts/princesse_xyz/*
type = discover

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M

[default]
default_calendar = personal
