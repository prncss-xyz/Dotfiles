[general]
status_path = "~/.vdirsyncer/status/"

[pair gmail_quarknoir_contacts]
a = "gmail_quarknoir_contacts_local"
b = "gmail_quarknoir_contacts_remote"
collections = ["from a", "from b"]

[storage gmail_quarknoir_contacts_local]
type = "google_contacts"
token_file = "..."
client_id = "..."
client_secret = "..."

type = "filesystem"
path = "~/.contacts/gmail_quarknoir/"
fileext = ".vcf"

[storage gmail_quarknoir_contacts_remote]
type = "carddav"

url = "https://www.google.com/contacts/dav/"
username = "quarknoir"
password.fetch = ["command", "pass", "google.com/quarknoir@gmail.com"]

