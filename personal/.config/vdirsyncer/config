[general]
status_path = "~/.vdirsyncer/status/"

[pair princesse_xyz_calendars]
a = "princesse_xyz_calendars_local"
b = "princesse_xyz_calendars_remote"
collections = ["from a", "from b"]

[storage princesse_xyz_calendars_local]
