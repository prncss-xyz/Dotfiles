path = "~/Media/calendars/princesse_xyz/"
fileext = ".ics"

[storage princesse_xyz_calendars_remote]
type = "caldav"

url = "https://webmail.gandi.net/SOGo/dav/princesse@princesse.xyz/Calendar/personal/"
username = "princesse@princesse.xyz"
password.fetch = ["command", "pass", "webmail.gandi.net/princesse@princesse.xyz"]

